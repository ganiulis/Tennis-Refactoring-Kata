{
    "name": "ganiulis/tennis-refactoring-kata-php",
    "description": "Tennis Refactoring Kata in PHP",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.6",
        "phpstan/phpstan-phpunit": "^1.1",
        "symplify/easy-coding-standard": "^10.2",
        "symplify/phpstan-extensions": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
